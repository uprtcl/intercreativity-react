    "@babel/preset-react": "^7.10.1",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
