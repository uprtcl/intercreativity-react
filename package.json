    "@uprtcl/http-provider": "0.1.0-beta.7",
    "@uprtcl/lenses": "0.1.0-beta.4",
    "@uprtcl/micro-orchestrator": "0.1.0-beta.4",
    "@uprtcl/multiplatform": "0.1.0-beta.4",
    "apollo-boost": "^0.4.9",
    "ipfs": "^0.50.2",
    "source-map-loader": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
